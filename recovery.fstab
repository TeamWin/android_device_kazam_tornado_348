# mount point	fstype		device			[device2]

/boot		emmc		/dev/bootimg
/cache		ext4		/dev/block/mmcblk0p7
/data		ext4		/dev/block/mmcblk0p8
/recovery	emmc		/dev/recovery
/system		ext4		/dev/block/mmcblk0p6
/sdcard 	vfat		/dev/block/mmcblk0p9				flags=display="Internal Storage";settingsstorage;wipeingui
/usb-otg      vfat          /dev/block/sda1	/dev/block/sda	flags=removable;storage;display=USB-OTG
